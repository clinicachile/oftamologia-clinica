<tr id="<%= dom_id pacient %>">
  <th scope="row"> <%= "#{pacient.first_name} #{pacient.last_name}" %> </th>
  <td> <%= pacient.run %> </td>
  <td> <%= pacient.age %> </td>
  <td> <%= pacient.phone %> </td>
  <td> <%= pacient.email %> </td>
  <td> 
    
  <div class="btn-group">
  <button type="button" class="btn btn-danger dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
    Acciones
  </button>
  <ul class="dropdown-menu">
   
    <li> <%= link_to "Modificar Paciente",edit_admin_pacient_path(pacient), class:'dropdown-item', data: { turbo_frame: :modal} %> </li>
    <li> <%= link_to "Obtener formato", admin_pacient_path(pacient,format: :pdf),class:'dropdown-item'  %> </li>
    <li> <%= link_to "Eliminar Paciente",admin_pacient_path(pacient), class:'dropdown-item', data: {turbo_method: :delete} %> </li>
  </ul>
</div>

  </td>
</tr>
